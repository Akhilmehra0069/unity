<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Protected</customSettingsVisibility>
    <description>All custom settings for the Community will be stored within this object.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Events_Allow_Group_Events__c</fullName>
        <defaultValue>true</defaultValue>
        <description>An organization can choose to allow events to be created for chatter groups.</description>
        <externalId>false</externalId>
        <label>Events - Allow Group Events</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Events_Allow_Leader_Group_Events_Only__c</fullName>
        <defaultValue>false</defaultValue>
        <description>An organization can determine whether all Group Members may create Group Events, or whether only Group Leaders may create Events for the Group.</description>
        <externalId>false</externalId>
        <label>Events - Allow Leader Group Events Only</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Group_Tiny_Goup_Limit__c</fullName>
        <defaultValue>7</defaultValue>
        <description>Defines the maximum number of users who can be added to a Tiny Group, which is used for Project/Study Groups and direct collaboration. These records are not full Chatter groups.</description>
        <externalId>false</externalId>
        <label>Group Tiny Goup Limit</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Group_Tiny_Group_Values__c</fullName>
        <defaultValue>&apos;Project/Study Group&apos;</defaultValue>
        <description>Defines values of Group_Control__c.Purpose__c that will always create a Tiny Group.  Separate values with ;</description>
        <externalId>false</externalId>
        <inlineHelpText>Separate values with ;</inlineHelpText>
        <label>Group - Tiny Group Values</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>News_Max_Sticky__c</fullName>
        <description>An organization can define a maximum number of &quot;sticky&quot; (featured) Community News records that are actively published the Community.</description>
        <externalId>false</externalId>
        <label>News Max Sticky</label>
        <precision>1</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Community Settings</label>
</CustomObject>
